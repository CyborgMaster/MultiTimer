{
  "name": "Multi-Timer",
  "description": "Lot's of way to track time",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "page": "background.html",
      "persistent": true
    }
  },
  "icons": { "16": "calculator-16.png", "128": "calculator-128.png" },
  "permissions": ["notifications"],
  "sandbox": {
    "pages": ["sandbox.html"]
  }

}
